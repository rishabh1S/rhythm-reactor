{
  "expo": {
    "name": "rhythm",
    "slug": "rhythm-reactor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/rhythm.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/rhythm.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.rishabh1s.rhythmreactor"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.ico"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6ca24253-571e-49e8-b310-58eb1e4a18ba"
      }
    }
  }
}
